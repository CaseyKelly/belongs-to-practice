
<table>
  <tr>
    <th> First Name </th>
    <th> Last Name </th>
    <th> Email </th>
    <th></th>
    <th></th>
  </tr>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.first_name, user %> </td>
        <td><%= user.last_name %> </td>
        <td><%= user.email %> </td>
        <td><%= link_to "Create Dog", new_user_dog_path(user) %> </td>
      </tr>
      <% end %>
      </tbody>
</table>
